{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-console": "off",
    //"no-console": ["error", { "allow": ["warn", "error"] }]
    "linebreak-style": [
      "error",
      "unix, windows"
    ]
  }
}